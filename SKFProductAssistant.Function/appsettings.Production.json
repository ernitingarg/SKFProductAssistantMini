{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Error",
      "Microsoft": "Error",
      "Microsoft.AspNetCore.Hosting.Internal.WebHost": "Error"
    }
  },
  "CacheProvider": "redis",
  "CacheConfig": {
    "AbsoluteExpirationRelativeToNowInMinutes": 43200,
    "SlidingExpirationInMinutes": 10080
  }
}
